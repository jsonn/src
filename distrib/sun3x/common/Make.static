#
# This is logically appended to some other makefile, i.e.
# using:  make -f Makefile -f $(OUTDIR)/Make.static ...
STRIP_CMD=strip

${OUTDIR}/${PROG} : ${OBJS} ${LIBC} ${DPADD}
	${CC} -static ${LDFLAGS} -o ${OUTDIR}/${PROG} ${OBJS} ${LDADD}
	${STRIP_CMD} ${OUTDIR}/${PROG}

