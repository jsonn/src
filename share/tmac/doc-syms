.\" NS Nx macro -- NetBSD
.de Nx
.nr cF \\n(.f
.nr cZ \\n(.s
.ds aa \&\f\\n(cF\s\\n(cZ
.if \\n(aC==0 \{\
.       ie \\n(.$==0 \&\\*(tNN\\s\\n(cZet\\*(tNBSD\\*(aa
.       el .aV \\$1 \\$2 \\$3 \\$4 \\$5 \\$6 \\$7 \\$8 \\$9
.\}
.if \\n(aC>\\n(aP \{\
.       nr aP \\n(aP+1
.	ie \\n(C\\n(aP==2 \{\
.		as b1 \&\\*(tNN\\s\\n(cZet\\*(tNBSD\ \&\\*(A\\n(aP\\*(aa
.		ie \\n(aC>\\n(aP \{\
.			nr aP \\n(aP+1
.			nR
.		\}
.		el .aZ
.	\}
.	el \{\
.	       as b1 \&\\*(tNN\\s\\n(cZet\\*(tNBSD\\*(aa
.	       nR
.	\}
.\}
