rdist		541/tcp		rdistd		# rdist daemon
afpovertcp	548/tcp				# AppleshareIP protocol
afpovertcp	548/udp				# AppleshareIP protocol
remotefs	556/tcp		rfs_server rfs	# Brunhoff remote filesystem
