#!/bin/sh
#
# kdc,v 1.6 2004/08/13 18:08:03 mycroft Exp
#

# PROVIDE: kdc
# REQUIRE: NETWORKING syslogd ntpdate
# BEFORE:  SERVERS

$_rc_subr_loaded . /etc/rc.subr

name="kdc"
rcvar=$name
command="/usr/sbin/${name}"
pidfile="/var/run/${name}.pid"
required_files="/etc/krb5.conf"

load_rc_config $name
run_rc_command "$1"
