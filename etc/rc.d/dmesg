#!/bin/sh
#
# $NetBSD: dmesg,v 1.3.4.2 2000/10/02 01:09:23 lukem Exp $
#

# PROVIDE: dmesg
# REQUIRE: mountcritremote

. /etc/rc.subr

name="dmesg"
rcvar=$name
start_cmd="do_dmesg"
stop_cmd=":"

do_dmesg()
{
	rm -f /var/run/dmesg.boot
	dmesg $dmesg_flags > /var/run/dmesg.boot
}

load_rc_config $name
run_rc_command "$1"
