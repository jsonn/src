#!/bin/sh
#
# $NetBSD: rbootd,v 1.4.4.1 2000/10/02 01:09:25 lukem Exp $
#

# PROVIDE: rbootd
# REQUIRE: DAEMON

. /etc/rc.subr

name="rbootd"
rcvar=$name
command="/usr/sbin/${name}"
pidfile="/var/run/${name}.pid"
required_files="/etc/${name}.conf"
extra_commands="reload"

load_rc_config $name
run_rc_command "$1"
