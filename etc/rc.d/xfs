#!/bin/sh
#
# $NetBSD: xfs,v 1.3.4.2 2000/11/10 00:01:46 tv Exp $
#

# PROVIDE: xfs
# REQUIRE: mountall cleartmp

. /etc/rc.subr

name="xfs"
rcvar=$name
command="/usr/X11R6/bin/${name}"
command_args="& sleep 2"
required_files="/usr/X11R6/lib/X11/fs/config"
extra_commands="reload"

load_rc_config $name
run_rc_command "$1"
