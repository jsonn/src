		if test $ol_enable_slapi = yes ; then
		{ echo "$as_me:$LINENO: WARNING: slapd disabled, ignoring --enable-slapi argument" >&5
echo "$as_me: WARNING: slapd disabled, ignoring --enable-slapi argument" >&2;}
