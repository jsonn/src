# list of subdirs

.if ${MACHINE} == "sparc64"
MD_SUBDIR=	sparc64/sparc
.endif

.if ${MACHINE} == "amd64"
MD_SUBDIR=	amd64/i386
.endif

.if (${MACHINE_ARCH} == "mips64eb" || ${MACHINE_ARCH} == "mips64el")
MD_SUBDIR=	mips64/64
.if target(obj)
MD_SUBDIR+=	.WAIT
.endif
MD_SUBDIR+=	mips64/o32
.endif
