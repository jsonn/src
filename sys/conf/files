define	atapi {[channel = -1]}
define  mii {[phy = -1]}

# audio device attributes
define	mulaw
define	auconv

# audio and midi devices, attaches to audio hardware driver
device	audio
attach	audio at audio
device	midi
attach	midi at midibus

