/*	$NetBSD: kmodule,v 1.1.4.2 2010/03/11 15:04:26 yamt Exp $	*/

SECTIONS
{
   /* Pre-loaded modules must have merged BSS and data. */
   .data 0 :
   {
     *(.data)
     *(.data.*)
     *(.bss)
     *(.bss.*)
     *(COMMON)
   }
   /* Pre-loaded modules do not need the following. */
   /DISCARD/ :
   {
     *(.comment)
   }
}
