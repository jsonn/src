all: acpi_wakecode.h

acpi_wakecode.h: acpi_wakecode.S genwakecode.sh
	${CC} -c acpi_wakecode.S
	objcopy -S -O binary acpi_wakecode.o acpi_wakecode.bin
	sh ${.CURDIR}/genwakecode.sh > acpi_wakecode.h
	rm -f acpi_wakecode.bin acpi_wakecode.o
