#
# "options" common to all mac68k kernel configs

machine	mac68k m68k
include		"conf/std"	# MI standard options

options 	HZ=60			# mac clock runs at 60hz

# Executable support
options 	EXEC_ELF32	# support for exec'ing ELF
options 	EXEC_SCRIPT	# support for #! scripts
