# standard, required pmax info

machine pmax mips

# Standard exec-package options
options 	EXEC_ELF32	# 32-bit ELF support (native format)
options 	EXEC_SCRIPT	# exec of #! scripts

makeoptions	DEFTEXTADDR="0x80030000"
makeoptions	ENDIAN="-EL"
