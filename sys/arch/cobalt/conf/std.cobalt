machine cobalt mips

options 	MIPS3
options 	MIPS3_5200
options 	MIPS3_L2CACHE_ABSENT

options 	EXEC_ELF32	# exec ELF32 binaries
options 	EXEC_SCRIPT	# exec #! scripts

options 	__NO_SOFT_SERIAL_INTERRUPT

makeoptions	DEFTEXTADDR="0x80001000"
makeoptions	MACHINE_ARCH="mipsel"
