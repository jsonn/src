S?=	${.CURDIR}/../../../..

.PHONY:	machine-links

machine-links:
	-rm -f machine && \
	ln -s $S/arch/${MACHINE}/include machine
	-rm -f ${MACHINE_ARCH} && \
	ln -s $S/arch/${MACHINE_ARCH}/include ${MACHINE_ARCH}

CLEANFILES+=	machine ${MACHINE_ARCH}

beforedepend:	machine-links

