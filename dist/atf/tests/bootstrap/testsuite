    echo 'Not enabling shell tracing (command contains a ${...} parameter expansion)'
fi

if test -n "$at_trace_this"; then
    ( $at_traceon; $({
    old=$(pwd)
    while test $(pwd) != '/' -a ! -e ./Makefile; do
        cd ..
    done
    topdir=$(pwd)
    cd ${old}
    echo ${topdir}
})/tests/bootstrap/h_${h} \
