    at_status=$?
    grep '^ *+' "$at_stder1" >&2
    grep -v '^ *+' "$at_stder1" >"$at_stderr"
else
    ( :; COLUMNS=80 call_atf_format <text ) >"$at_stdout" 2>"$at_stderr"
    at_status=$?
fi

