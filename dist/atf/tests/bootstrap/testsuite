cat >top/test1 <<EOF
#! $(atf-config -t atf_shell)
echo "Content-Type: application/X-atf-tcs; version=\"1\"" >&9
echo "" >&9
echo 'tcs-count: 1' >&9
echo 'tc-start: tc_1' >&9
echo ignore-me
echo ignore-me >&2
echo 'tc-end: tc_1, passed' >&9
true
EOF
$at_traceoff
