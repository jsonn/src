.SUFFIXES:
$(srcdir)/Makefile.in: Makefile.am $(top_srcdir)/$(altdir)configure.in $(ACLOCAL_M4) 
	cd $(top_srcdir) && $(AUTOMAKE) --altdir=$(altdir) --gnu Makefile

Makefile: $(srcdir)/Makefile.in  $(top_builddir)/config.status $(BUILT_SOURCES)
	cd $(top_builddir) \
	  && CONFIG_FILES=$@ CONFIG_HEADERS= $(SHELL) ./config.status

$(ACLOCAL_M4):  ./aux/configure.in  ./aux/acinclude.m4
	cd $(srcdir) && $(ACLOCAL) --altdir=$(altdir)

config.status: $(srcdir)/configure $(CONFIG_STATUS_DEPENDENCIES)
	$(SHELL) ./config.status --recheck
$(srcdir)/configure: $(srcdir)/$(altdir)configure.in $(ACLOCAL_M4) $(CONFIGURE_DEPENDENCIES)
	cd $(srcdir) && $(AUTOCONF) --localdir=$(altdir) ./aux/configure.in > configure

config.h: stamp-h
	@if test ! -f $@; then \
		rm -f stamp-h; \
		$(MAKE) stamp-h; \
	else :; fi
stamp-h: $(srcdir)/aux/config.h.in $(top_builddir)/config.status
	@rm -f stamp-h stamp-hT
	@echo timestamp > stamp-hT 2> /dev/null
	cd $(top_builddir) \
	  && CONFIG_FILES= CONFIG_HEADERS=config.h:aux/config.h.in \
	     $(SHELL) ./config.status
	@mv stamp-hT stamp-h
$(srcdir)/aux/config.h.in: $(srcdir)/aux/stamp-h.in
	@if test ! -f $@; then \
		rm -f $(srcdir)/aux/stamp-h.in; \
		$(MAKE) $(srcdir)/aux/stamp-h.in; \
	else :; fi
$(srcdir)/aux/stamp-h.in: $(top_srcdir)/$(altdir)configure.in $(ACLOCAL_M4) 
	@rm -f $(srcdir)/aux/stamp-h.in $(srcdir)/aux/stamp-h.inT
	@echo timestamp > $(srcdir)/aux/stamp-h.inT 2> /dev/null
	cd $(top_srcdir) && $(AUTOHEADER) --localdir=$(altdir) > aux/config.h.in
	@mv $(srcdir)/aux/stamp-h.inT $(srcdir)/aux/stamp-h.in

